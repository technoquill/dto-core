{
  "name": "technoquill/dto-core",
  "description": "Lightweight typed DTO base for PHP projects with fillable and static factory support",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Technoquill\\DTO\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/error-handler": "^6.1 || ^7.0"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.2",
    "phpunit/phpunit": "^9.5"
}

}
